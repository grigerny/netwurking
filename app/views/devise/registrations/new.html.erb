<div class="row-fluid">
	<div class="woodsection">
		<div class="span12">
			

			
			<div class="row-fluid">
				<div class="span4 offset4">
					<div class="signupwrapper">


<div class="usersignupemail">						

<div class="row-fluid">
		<div class="signuptop">
			Sign up
		</div>	
</div>							


<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

<div class="row-fluid">
  <%= f.email_field :email, :placeholder => "Email Address", :class => "usersignupemail2" %>
</div>

<div class="row-fluid">
  <%= f.password_field :password, :placeholder => "Password", :class => "usersignupemail2" %>
</div>

<div class="row-fluid">
<%= f.label :password_confirmation%>
</div>

<div class="row-fluid">
  <%= f.password_field :password_confirmation, :placeholder => "Password", :class => "usersignupemail2" %>
</div>

<div class="row-fluid">
<%= f.submit "Sign up", :class => "usersignupsubmit" %>
<% end %>
</div>

<%= render "devise/shared/links" %>
</div>
					
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
